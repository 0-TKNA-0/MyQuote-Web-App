<div id="<%= dom_id user %>">
  <p>
    <strong>Ufname:</strong>
    <%= user.ufname %>
  </p>

  <p>
    <strong>Ulname:</strong>
    <%= user.ulname %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Current Password:</strong> <span class="text-danger">ENCRYPTED</span>
    <% user.password_digest %>
  </p>

  <p>
    <strong>Is admin:</strong>
    <%= user.is_admin %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= user.status %>
  </p>

</div>
